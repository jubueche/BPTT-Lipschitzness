{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": [],
            "console": "integratedTerminal"
        },
        {
            "name": "ESGD",
            "type": "python",
            "request": "launch",
            "justMyCode": true,
            "program": "${file}",
            "args": ["-optimizer=esgd", "-beta_robustness=0.0", "-learning_rate=0.1,0.01"],
            "console": "integratedTerminal"
        },
        {
            "name": "SGD",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": ["-exp=sgd_experiment", "-mode=bsub"],
            "console": "integratedTerminal"
        },
        {
            "name": "Speech",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": ["-beta_robustness=0.0", "-dropout_prob=0.3", "-wanted_words=yes,no,up,down,left,right"],
            "console": "integratedTerminal"
        },
        {
            "name": "ECG",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": ["-beta_robustness=0.0", "-dropout_prob=0.3"],
            "console": "integratedTerminal"
        },
        {
            "name": "CNN",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": ["-beta_robustness=0.0", "-dropout_prob=0.3"],
            "console": "integratedTerminal"
        },
        {
            "name": "l2 boundary test",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": ["-boundary_loss=l2"],
            "console": "integratedTerminal"
        }
    ]
}
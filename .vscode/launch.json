{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": [],
            "console": "integratedTerminal"
        },
        {
            "name": "ESGD",
            "type": "python",
            "request": "launch",
            "justMyCode": true,
            "program": "${file}",
            "args": ["-optimizer=esgd", "-beta_robustness=0.0", "-learning_rate=0.1,0.01"],
            "console": "integratedTerminal"
        },
        {
            "name": "SGD",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": ["-exp=sgd_experiment", "-mode=bsub"],
            "console": "integratedTerminal"
        },
        {
            "name": "Speech",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": ["-beta_robustness=0.0", "-attack_size_mismatch=0.3", "-n_epochs=10,100,50", "-learning_rate=0.01,0.001,0.0001", "-initial_std_mismatch=0.001", "-seed=0", "-wanted_words=yes,no,up,down,left,right", "-contractive=1.0", "-contractive_params=[W_rec]"],
            "console": "integratedTerminal"
        },
        {
            "name": "ECG",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": ["-beta_robustness=0.0", "-l2_weight_decay=0.1", "-l2_weight_decay_params=[W_rec]", "-contractive=0.1", "-contractive_params=[W_rec]"],
            "console": "integratedTerminal"
        },
        {
            "name": "CNN",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": ["-beta_robustness=0.0", "-l2_weight_decay=0.1", "-l2_weight_decay_params=[W2]", "-contractive=0.1", "-contractive_params=[W2]"],
            "console": "integratedTerminal"
        },
        {
            "name": "l2 boundary test",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": ["-boundary_loss=l2"],
            "console": "integratedTerminal"
        }
    ]
}